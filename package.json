{
  "name": "heimdall",
  "version": "3.0.0",
  "private": true,
  "description": "An application management portal",
  "author": "KodeStar <kodestar@gmail.com>",
  "contributors": [
    "Alex Phillips <ahp118@gmail.com>",
    "Eric Nemchik <eric@nemchik.com>"
  ],
  "scripts": {
    "build": "npm run format:write && quasar build",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "dev": "quasar dev",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint --ext .js,.vue src",
    "server": "npm run db:migrate && node ./bin/www",
    "setup": "npm install && node ./scripts/install && npm run build && npm run db:migrate && npm run db:seed",
    "pretest": "NODE_ENV=test npm run db:migrate",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,htm,html,js,json,jsx,md,php,scss,ts,tsx,vue,yaml,yml}": [
      "prettier --write"
    ],
    "{.prettierrc,.sequelizerc,.stylintrc,bin/www}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "dependencies": {
    "@quasar/extras": "^1.10.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.10.1",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dockerode": "^3.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^16.3.0",
    "inquirer": "^8.1.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.30",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.4.4",
    "quasar": "^1.15.10",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.0.2",
    "vue-async-computed": "^3.9.0",
    "vue-i18n": "^8.24.3",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@quasar/app": "^2.2.4",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.10",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "productName": "Heimdall"
}
