{
  "name": "heimdall",
  "version": "3.0.0",
  "private": true,
  "description": "An application management portal",
  "author": "KodeStar <kodestar@gmail.com>",
  "contributors": [
    "Alex Phillips <ahp118@gmail.com>",
    "Eric Nemchik <eric@nemchik.com>"
  ],
  "scripts": {
    "build": "npm run format:write && quasar build",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "dev": "quasar dev",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint --ext .js,.vue src",
    "server": "npm run db:migrate && node ./bin/www",
    "setup": "npm install && node ./scripts/install && npm run build && npm run db:migrate && npm run db:seed",
    "pretest": "NODE_ENV=test npm run db:migrate",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,htm,html,js,json,jsx,md,php,scss,ts,tsx,vue,yaml,yml}": [
      "prettier --write"
    ],
    "{.prettierrc,.sequelizerc,.stylintrc,bin/www}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "dependencies": {
    "@quasar/extras": "^1.7.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dockerode": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^14.2.0",
    "inquirer": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.27",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.4.4",
    "quasar": "^1.13.0",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^4.2.0",
    "vue-async-computed": "^3.8.2",
    "vue-i18n": "^8.17.4",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.8.4",
    "@quasar/quasar-app-extension-dotenv": "^1.0.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^25.5.2",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "prettier-plugin-packagejson": "^2.2.2",
    "sequelize-cli": "^5.5.1",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "capacitorId": "",
  "cordovaId": "site.heimdall.app",
  "productName": "Heimdall"
}
